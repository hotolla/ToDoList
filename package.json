{
  "name": "to-do-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@mui/styles": "^5.8.6",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "axios": "^0.27.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "server": "json-server --watch db.json --port 3002",
    "dev": "concurrently \"npm run server\" \"npm start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "max-len": [
        "warn",
        {
          "code": 120
        }
      ],
      "array-bracket-spacing": [
        "warn",
        "always"
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "arrow-parens": [
        "warn",
        "always"
      ],
      "semi": "warn",
      "no-alert": "warn",
      "no-console": "warn",
      "no-debugger": "warn",
      "comma-dangle": [
        "warn",
        "never"
      ],
      "import/no-anonymous-default-export": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.1",
    "@types/mocha": "^9.1.1",
    "concurrently": "^7.2.2",
    "json-server": "^0.17.0"
  }
}
